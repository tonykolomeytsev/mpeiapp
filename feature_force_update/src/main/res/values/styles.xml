<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ForceUpdateButtonUpdate">
        <item name="android:background">@drawable/force_update_button</item>
        <item name="android:backgroundTint">@color/colorSecondary</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
    </style>

    <style name="ForceUpdateButtonLater">
        <item name="android:background">@drawable/force_update_button</item>
        <item name="android:backgroundTint">?attr/themeColorForceUpdateBtnLater</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/themeColorBlack</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
    </style>

</resources>