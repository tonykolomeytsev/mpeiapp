<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="navigationIcon">@drawable/ic_arrow_back</item>
        <item name="titleTextAppearance">@style/H3</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>

    <style name="Toolbar.Close">
        <item name="navigationIcon">@drawable/ic_close_black</item>
    </style>

    <style name="Toolbar.WebView">
        <item name="titleTextAppearance">@style/H4</item>
        <item name="subtitleTextAppearance">@style/H7.Gray70</item>
        <item name="navigationIcon">@drawable/ic_close_black</item>
    </style>

    <style name="EditText" parent="Widget.AppCompat.EditText"/>

</resources>